<view class="mx-2"><view class=" flex align-center " style="height:100rpx;"><view data-event-opts="{{[['tap',[['changeTab',[index]]]]]}}" class="{{(((((('flex-1')+' '+'flex')+' '+'align-center')+' '+'justify-center')+' '+'')+' '+(index===tabIndex?'font-md text-main font-weight-bold ':'font'))}}" onTap="__e" a:for="{{tabBars}}" a:for-item="item" a:for-index="index" a:key="index">{{''+item.name+''}}<block a:if="{{item.mun>0}}"><text class="ml-2 font">{{item.mun}}</text></block></view></view><swiper style="{{('height:'+scrollH+'px;')}}" duration="{{150}}" current="{{tabIndex}}" data-event-opts="{{[['change',[['onChangeTab',['$event']]]]]}}" onChange="__e"><swiper-item a:for="{{newList}}" a:for-item="item" a:for-index="index" a:key="index"><scroll-view style="{{('height:'+scrollH+'px;')}}" scroll-y="true" data-event-opts="{{[['scrollToLower',[['loadmore',[index]]]]]}}" onScrollToLower="__e"><block a:if="{{item.list.length>0}}"><block a:for="{{item.list}}" a:for-item="item2" a:for-index="index2" a:key="index2"><friends-item vue-id="{{'093c75d6-1-'+index+'-'+index2}}" item="{{item2}}" index="{{index}}" onVueInit="__l"></friends-item></block><block a:if="{{item.list.length>10}}"><load-more vue-id="{{'093c75d6-2-'+index}}" loadmore="{{item.loadMore}}" onVueInit="__l"></load-more></block></block><block a:else><empty-page vue-id="{{'093c75d6-3-'+index}}" text1="空空如也" text2="快去结交一些朋友吧" onVueInit="__l"></empty-page></block></scroll-view></swiper-item></swiper></view>