<view><scroll-view class="scroll-row" scroll-x="true" scroll-into-view="{{scrollInto}}" scroll-with-animation="{{true}}"><view class="{{((((('scroll-row-item')+' '+'px-3')+' '+'py-1')+' '+'font-md')+' '+(tabIndex===index?'text-main font-lg font-weight-bold':''))}}" style="height:60rpx;" id="{{'tab'+index}}" data-event-opts="{{[['tap',[['changeTab',[index]]]]]}}" onTap="__e" a:for="{{tablist}}" a:for-item="item" a:for-index="index" a:key="index">{{item.name}}</view></scroll-view><my-hr vue-id="470c2fd6-1" onVueInit="__l"></my-hr><swiper style="{{('height:'+scrollH+'px;')}}" duration="{{150}}" current="{{tabIndex}}" data-event-opts="{{[['change',[['onChangeTab',['$event']]]]]}}" onChange="__e"><swiper-item a:for="{{newList}}" a:for-item="item" a:for-index="index" a:key="index"><scroll-view style="{{('height:'+scrollH+'px;')}}" scroll-y="true" data-event-opts="{{[['scrollToLower',[['loadmore',[index]]]]]}}" onScrollToLower="__e"><block a:if="{{item.list.length>0}}"><block a:for="{{item.list}}" a:for-item="item2" a:for-index="index2" a:key="index2"><topic-item vue-id="{{'470c2fd6-2-'+index+'-'+index2}}" item="{{item2}}" index="{{index2}}" onVueInit="__l"></topic-item></block><load-more vue-id="{{'470c2fd6-3-'+index}}" loadmore="{{item.loadMore}}" onVueInit="__l"></load-more></block><block a:else><empty-page vue-id="{{'470c2fd6-4-'+index}}" text1="空空如也" onVueInit="__l"></empty-page></block></scroll-view></swiper-item></swiper></view>