<view class="uni-collapse-item" style="background-color:#f0f2f5;"><view data-event-opts="{{[['tap',[['onClick',[!isOpen]]]]]}}" class="{{((('uni-collapse-item__title')+' '+((isOpen&&titleBorder==='auto')?'is-open':''))+' '+((titleBorder!=='none')?'uni-collapse-item-border':''))}}" onTap="__e"><view class="uni-collapse-item__title-wrap"><block a:if="{{$slots.title}}"><slot name="title"></slot></block><block a:else><view class="{{(('uni-collapse-item__title-box')+' '+((disabled)?'is-disabled':''))}}"><block a:if="{{thumb}}"><image class="uni-collapse-item__title-img" src="{{thumb}}"></image></block><text class="uni-collapse-item__title-text">{{title}}</text></view></block></view><block a:if="{{showArrow}}"><view class="{{((('uni-collapse-item__title-arrow')+' '+((isOpen)?'uni-collapse-item__title-arrow-active':''))+' '+((showAnimation===true)?'uni-collapse-item--animation':''))}}"><uni-icons vue-id="4178e9b0-1" color="{{disabled?'#ddd':'#bbb'}}" size="14" type="bottom" onVueInit="__l"></uni-icons></view></block></view><view class="{{(('uni-collapse-item__wrap')+' '+((showAnimation)?'is--transition':''))}}" style="{{'height:'+((isOpen?height:0)+'px')+';'}}"><view class="{{((('uni-collapse-item__wrap-content')+' '+((isheight)?'open':''))+' '+((border&&isOpen)?'uni-collapse-item--border':''))}}" id="{{elId}}" ref="__r" data-ref="collapse--hook"><slot></slot></view></view></view>