<view class="uni-file-picker__container"><view class="file-picker__box" style="{{(boxStyle)}}" a:for="{{filesList}}" a:for-item="item" a:for-index="index" a:key="index"><view class="file-picker__box-content" style="{{(borderStyle)}}"><image class="file-image" src="{{item.url}}" mode="aspectFill" data-event-opts="{{[['tap',[['prviewImage',['$0',index],[[['filesList','',index]]]]]]]}}" catchTap="__e"></image><block a:if="{{delIcon&&!readonly}}"><view data-event-opts="{{[['tap',[['delFile',[index]]]]]}}" class="icon-del-box" catchTap="__e"><view class="icon-del"></view><view class="icon-del rotate"></view></view></block><block a:if="{{item.progress&&item.progress!==100||item.progress===0}}"><view class="file-picker__progress"><progress class="file-picker__progress-item" percent="{{item.progress===-1?0:item.progress}}" stroke-width="4" backgroundColor="{{item.errMsg?'#ff5a5f':'#EBEBEB'}}"></progress></view></block><block a:if="{{item.errMsg}}"><view data-event-opts="{{[['tap',[['uploadFiles',['$0',index],[[['filesList','',index]]]]]]]}}" class="file-picker__mask" catchTap="__e">点击重试</view></block></view></view><block a:if="{{filesList.length<limit&&!readonly}}"><view class="file-picker__box" style="{{(boxStyle)}}"><view data-event-opts="{{[['tap',[['choose',['$event']]]]]}}" class="file-picker__box-content is-add" style="{{(borderStyle)}}" onTap="__e"><block a:if="{{$slots.$default}}"><slot></slot></block><block a:else><view class="icon-add"></view><view class="icon-add rotate"></view></block></view></view></block></view>